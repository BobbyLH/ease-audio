{
  "name": "@xmly/ease-audio",
  "version": "0.2.5",
  "description": "The Audio Player Project",
  "keywords": [
    "ease",
    "ease-audio",
    "ease-audio.js",
    "audio",
    "sound",
    "html5",
    "html5 audio"
  ],
  "main": "dist/ease-audio.js",
  "types": "dist/ease-audio.d.ts",
  "scripts": {
    "dev": "npm run build_tsc && webpack-dashboard -p 3300 -- node demo/server.js",
    "test": "npm run build_src && karma start",
    "static": "rollup -c build/rollup.config.js --watch",
    "build": "npm run build_branch && npm run build_src && npm run build_test && npm run build_version && npm run build_git",
    "build:beta": "npm run build_branch && npm run build_src && npm run build_test && npm run build_version_beta && npm run build_git",
    "build_branch": "node build/branch",
    "build_version": "node build/version",
    "build_version_beta": "node build/version.beta",
    "build_git": "node build/git.js master",
    "build_test": "karma start --single-run",
    "build_src": "npm run build_tsc && npm run build_rollup && npm run build_webpack && npm run build_d_copy",
    "build_d_copy": "node build/copy.js",
    "build_tsc": "tsc --build tsconfig.json",
    "build_webpack": "webpack --config build/webpack.config.js",
    "build_rollup": "rollup -c build/rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:BobbyLH/ease-audio.git"
  },
  "author": "Bobby.Li",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-arrow-functions": "^7.2.0",
    "@babel/plugin-transform-block-scoping": "^7.2.0",
    "@babel/plugin-transform-classes": "^7.2.2",
    "@babel/plugin-transform-destructuring": "^7.2.0",
    "@babel/plugin-transform-function-name": "^7.2.0",
    "@babel/plugin-transform-instanceof": "^7.2.0",
    "@babel/plugin-transform-parameters": "^7.2.0",
    "@babel/plugin-transform-property-mutators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/plugin-transform-shorthand-properties": "^7.2.0",
    "@babel/plugin-transform-spread": "^7.2.2",
    "@babel/plugin-transform-template-literals": "^7.2.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.2.0",
    "core-js": "^2.6.1",
    "express": "^4.17.3",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-webpack": "^3.0.5",
    "mocha": "^6.1.4",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.2.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "shelljs": "^0.8.3",
    "simple-git": "^1.107.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.4",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dashboard": "^2.0.0",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  }
}
